<html lang="en-US">
<head>
    <title>Survey Analysis</title>
</head>

<body>
    <h1>Survey analysis</h1>
    <h2>Name</h2>
    <ul>
    <% for (var i=0; i<results[0].length; i++){ %>
        <li><%= results[0][i].name %> :  <%= results[0][i].count %></li>
    <% } %>
    </ul>
    <h2>Live Alone</h2>
    <ul>
    <% for (var i=0; i<results[1].length; i++){ %>
        <li><%= results[1][i].liveAlone %> :  <%= results[1][i].count %></li>
    <% } %>
    </ul>
    <h2>Members</h2>
    <ul>
    <% for (var i=0; i<results[2].length; i++){ %>
        <li><%= results[2][i].members %> :  <%= results[2][i].count %></li>
    <% } %>
    </ul>
    <h2>Pets</h2>
    <ul>
    <% for (var i=0; i<results[3].length; i++){ %>
        <li><%= results[3][i].pets %> :  <%= results[3][i].count %></li>
    <% } %>
    </ul>
    <h2>Likelihood of Filling Survey (1-5)</h2>
    <ul>
    <% for (var i=0; i<results[4].length; i++){ %>
        <li><%= results[4][i].fillSurvey %> :  <%= results[4][i].count %></li>
    <% } %>
    </ul>
    <h2>Names</h2>
    <ul>
    <% for (var i=0; i<results[5].length; i++){ %>
        <li><%= results[5][i].names %> :  <%= results[5][i].count %></li>
    <% } %>
    </ul>

</body>
</html>
